{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "chrome",
            "request": "launch",
            "name": "vuejs: chrome",
            "url": "http://localhost:8002",
            "webRoot": "${workspaceFolder}/src",
            "breakOnLoad": true,
            "sourceMapPathOverrides": {
                "webpack:///src/*": "${webRoot}/*"
            }
        },
        {
            "type": "firefox",
            "request": "launch",
            "name": "vuejs: firefox",
            "url": "http://localhost:8002",
            "webRoot": "${workspaceFolder}/src",
            "pathMappings": [
                {
                    "url": "webpack://admin-/[name]/src/components/page/field/views/grid/action.vue",
                    "path": "${workspaceFolder}/src/pages/_admin/frameworkuser/views/action.vue"
                },
                {
                    "url": "webpack:///src/",
                    "path": "${webRoot}/"
                }
            ]
        }
    ]
  }