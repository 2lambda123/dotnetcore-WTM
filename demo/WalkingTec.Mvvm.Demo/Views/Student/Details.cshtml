@model WalkingTec.Mvvm.Demo.ViewModels.StudentVMs.StudentVM
@inject IStringLocalizer<Program> Localizer;

<wt:form vm="@Model">
<wt:row items-per-row="ItemsPerRowEnum.Two">
<wt:display field="Entity.ID" />
<wt:display field="Entity.Password" />
<wt:display field="Entity.Email" />
<wt:display field="Entity.Name" />
<wt:display field="Entity.Sex" />
<wt:display field="Entity.CellPhone" />
<wt:display field="Entity.Address" />
<wt:display field="Entity.ZipCode" />
<wt:upload field="Entity.PhotoId" disabled="true" upload-type="ImageFile" width="300"/>
<wt:display field="Entity.IsValid" />
<wt:display field="Entity.EnRollDate" />

</wt:row>
    <wt:row align="AlignEnum.Right">
        <wt:closebutton />
    </wt:row>
</wt:form>
