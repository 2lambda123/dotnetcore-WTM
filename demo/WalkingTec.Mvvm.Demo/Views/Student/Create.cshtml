@model WalkingTec.Mvvm.Demo.ViewModels.StudentVMs.StudentVM
@inject IStringLocalizer<Program> Localizer;

<wt:form vm="@Model">
  <wt:row items-per-row="ItemsPerRowEnum.Two">
    <wt:textbox field="Entity.ID" />
    <wt:textbox field="Entity.Password" />
    <wt:textbox field="Entity.Email" />
    <wt:textbox field="Entity.Name" />
    <wt:combobox field="Entity.Sex" />
    <wt:textbox field="Entity.CellPhone" />
    <wt:textbox field="Entity.Address" />
    <wt:textbox field="Entity.ZipCode" />
    <wt:upload field="Entity.PhotoId"  upload-type=" ImageFile" />
    <wt:switch field="Entity.IsValid" default-value="true"/>
    <wt:datetime field="Entity.EnRollDate" />
      <wt:selector list-vm="MajorList" field="Entity.StudentMajor[-1].MajorId" text-bind="MajorList.EntityList[-1].MajorName" val-bind="MajorList.EntityList[-1].ID" multi-select="true">
    <wt:searchpanel vm="MajorList">
      <wt:row items-per-row=" ItemsPerRowEnum.Two">
        <wt:combobox field="MajorList.Searcher.SchoolId" items="MajorList.Searcher.AllSchools" />
      </wt:row>
    </wt:searchpanel>
  </wt:selector>
  </wt:row>
    <wt:row align="AlignEnum.Right">
        <wt:submitbutton />
        <wt:closebutton />
    </wt:row>
</wt:form>
