@*<wt:chart field="@Model.charts" type="Bar" title="abcd" height="400"/>
  <wt:chart field="@Model.charts" type="Pie" title="abcd" height="400" />*@

<wt:row items-per-row="ItemsPerRowEnum.Two">
  <wt:card title="圆环">
    <wt:chart  type="PieHollow" title="圆环" height="400" theme="WalkingTec.Mvvm.TagHelpers.LayUI.Chart.ChartThemeEnum.westeros" trigger-url="/DataTable/GetCharts" />
  </wt:card>
  <wt:card title="饼图">
    <wt:chart  type="Pie" title="饼图" height="400" theme="WalkingTec.Mvvm.TagHelpers.LayUI.Chart.ChartThemeEnum.dark" trigger-url="/DataTable/GetCharts" />
  </wt:card>
</wt:row>
<wt:row items-per-row="ItemsPerRowEnum.One">
  <wt:card title="散点图">
    <wt:chart is-horizontal="true" type="Scatter" title="散点图" height="400" trigger-url="/DataTable/ " />
  </wt:card>
</wt:row>
<wt:row items-per-row="ItemsPerRowEnum.Two">
  <wt:card title="折线图">
    <wt:chart is-horizontal="false" show-legend="true" open-smooth="true" show-tooltip="true"  type="Line" title="折线图" height="400" theme="WalkingTec.Mvvm.TagHelpers.LayUI.Chart.ChartThemeEnum.chalk" trigger-url="/DataTable/GetCharts" />
  </wt:card>
  <wt:card title="柱状图">
    <wt:chart is-horizontal="false" show-legend="true" show-tooltip="true"  type="Bar" title="柱状图" height="400" theme="WalkingTec.Mvvm.TagHelpers.LayUI.Chart.ChartThemeEnum.essos" trigger-url="/DataTable/GetCharts" />
  </wt:card>
</wt:row>


